#include<stdio.h>
void main(){
    int n,max,pos;
    printf("Enter the size of an array :");
    scanf("%d\n",&n);
    int a[n];
    printf("Enter the array elements: \n");
    for(int i=0;i<=n;i++){
        scanf("%d",&a[i]);
        if(a[i] > max){
            max= a[i];
            pos = i;
        }
    }
    printf("Max num : %d \t Position :%d",max,pos);
   
}
